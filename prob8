#include <iostream>
using namespace std;

int main()
{
	int n, m1, m2, m3, m4, m5, m6, m7, m8, m9, m10;
	cout << "Enter a number: "; cin >> n;
	cout << "Multiplication table of " << n << ":" << endl;
	m1 = 1 * n;
	cout << "1 * " << n << " = " << m1 << endl;
	m2 = 2 * n;
	cout << "2 * " << n << " = " << m2 << endl;
	m3 = 3 * n;
	cout << "3 * " << n << " = " << m3 << endl;
	m4 = 4 * n;
	cout << "4 * " << n << " = " << m4 << endl;
	m5 = 5 * n;
	cout << "5 * " << n << " = " << m5 << endl;
	m6 = 6 * n;
	cout << "6 * " << n << " = " << m6 << endl;
	m7 = 7 * n;
	cout << "7 * " << n << " = " << m7 << endl;
	m8 = 8 * n;
	cout << "8 * " << n << " = " << m8 << endl;
	m9 = 9 * n;
	cout << "9 * " << n << " = " << m9 << endl;
	m10 = 10 * n;
	cout << "10 * " << n << " = " << m10 << endl;

	return 0;
}
